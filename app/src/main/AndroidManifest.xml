<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gdmtech.ambientequiz">

    <application
        android:allowBackup="true"
        android:icon="@drawable/button_onca"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TelaJogoInicial01"></activity>
        <activity android:name=".TelaJogoForca01" />
        <activity android:name=".TelaJogoQuiz01" />
        <activity android:name=".TelaMenu" />
        <activity android:name=".TelaAvatar" />
        <activity android:name=".TelaInicial" />
        <activity
            android:name=".FullscreenActivityTelaInicial"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_fullscreen_tela_inicial"
            android:parentActivityName=".MainActivity"
            android:theme="@style/FullscreenTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gdmtech.ambientequiz.MainActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>